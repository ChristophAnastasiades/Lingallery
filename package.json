{
  "name": "lingallery",
  "version": "1.1.25",
  "description": "A simple image gallery component for Vue.js",
  "author": "Christoph Anastasiades <info@lingulo.com>",
  "private": false,
  "license": "MIT",
  "main": "./dist/lingallery.common.js",
  "module": "./dist/lingallery.umd.js",
  "browser": {
    "./sfc": "./src/components/Lingallery.vue"
  },
  "keywords": [
    "vue",
    "lingallery",
    "image",
    "gallery",
    "thumbnail",
    "photo",
    "vuejs"
  ],
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChristophAnastasiades/Lingallery.git"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-bundle": "vue-cli-service build --target lib --name lingallery ./src/components/index.js",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "epic-spinners": "^1.0.4",
    "vue": "^2.5.21",
    "vue-async-computed": "^3.5.1",
    "vue2-touch-events": "^1.1.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.3.0",
    "@vue/cli-plugin-eslint": "^3.3.0",
    "@vue/cli-service": "^3.3.0",
    "vue-template-compiler": "^2.5.21"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "vue": {
    "runtimeCompiler": true
  }
}
